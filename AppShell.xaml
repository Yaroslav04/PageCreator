<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PageCreator.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PageCreator.Core.View"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Setting"
        ContentTemplate="{DataTemplate local:SettingPage}"
        Route="SettingPage" />

    <ShellContent
        Title="Page"
        ContentTemplate="{DataTemplate local:CreatePagePage}"
        Route="CreatePagePage" />

</Shell>
